<!doctype html>
<html>
  <head>
    {% include 'includes/header.html' %}
  </head>
  <body>
    {% include 'includes/status-console.html' %}
    {% include 'includes/spinner.html' %}

    <style>
    .text-large {
      font-size: 125%;
    }
    </style>

    {% include 'includes/jumbotron.html' %}

    <style>
    .carousel-control-prev-icon {
        background-image: url("data:image/svg+xml;charset=utf8,%3Csvg xmlns='http://www.w3.org/2000/svg' fill='%23000' viewBox='0 0 8 8'%3E%3Cpath d='M5.25 0l-4 4 4 4 1.5-1.5-2.5-2.5 2.5-2.5-1.5-1.5z'/%3E%3C/svg%3E");
    }

    .carousel-control-next-icon {
        background-image: url("data:image/svg+xml;charset=utf8,%3Csvg xmlns='http://www.w3.org/2000/svg' fill='%23000' viewBox='0 0 8 8'%3E%3Cpath d='M2.75 0l-1.5 1.5 2.5 2.5-2.5 2.5 1.5 1.5 4-4-4-4z'/%3E%3C/svg%3E");
    }

    .carousel-control-prev {
      position: absolute;
      top: auto;
      bottom: 36px;
      right: 85%;
    }

    .carousel-control-next{
      position: absolute;
      top: auto;
      bottom: 36px;
      left: 85%;
    }

    .carousel .carousel-indicators li {
      background-color: #fff;
      background-color: rgba(70, 70, 70, 0.25);
    }

    .carousel .carousel-indicators .active {
      background-color: #444;
    }
    </style>

    <div class="row p-3 container-fluid">
      <div class="col-sm-12 p-3 mx-auto">
        {% include 'includes/readme.html' %}
      </div>
    </div>

    <div class="row p-3 container-fluid">
    <div
      style="display: flex; align-self: stretch;"
      class="col-lg-8 mx-auto p-3"
    >
      <div
      style="position: -webkit-sticky; position: sticky; bottom: 0rem; align-self: flex-end;"
      class="w-100"
    >
            {% include 'includes/viewer.html' %}
            {% include 'includes/controls.html' %}
        </div>
    </div>
      <div class="col-lg-4 p-3 mx-auto">
        {% import "includes/view-key.html.jinja" as view_key %}
        {{ view_key.content(data, eval, make_badge, markdown) }}
        {% include 'includes/view-selector.html' %}
      </div>
    </div>

    <div class="row p-3 container-fluid">
      <div class="col-lg-6 p-3">
        {% include 'includes/systematics.html' %}
      </div>
      <div class="col-lg-6 p-3 mx-auto">
        {% include 'includes/dominant-viewer.html' %}
      </div>
    </div>

    <div class="row p-3 container-fluid">
      <div class="col-lg-12 p-3">
        {% include 'includes/population-loader.html' %}
      </div>
    </div>

    <div class="row p-3 container-fluid">
      <div class="col-lg-12 p-3">
        {% include 'includes/config.html' %}
      </div>
    </div>

    <div class="row p-3 container-fluid">
      <div class="col-lg-12 p-3">
        {% include 'includes/execution-log-panel.html' %}
      </div>
    </div>

    <div class="row p-3 container-fluid">
      <div class="col-md-6 col-xl-3 p-3">
        {% include 'includes/social.html' %}
      </div>
      <div class="col-md-6 col-xl-3 p-3">
        {% include 'includes/source-body.html' %}
      </div>
      <div class="col-md-6 col-xl-3 p-3">
        {% include 'includes/references-body.html' %}
      </div>
      <div class="col-md-6 col-xl-3 p-3">
        {% include 'includes/acknowledgements.html' %}
      </div>
    </div>

    <script type="text/javascript">
      const script = document.createElement('script')
      if (typeof SharedArrayBuffer !== 'undefined') {
        script.src = 'dishtiny.js';
      } else {
        script.src = 'dishtiny-no-pthread.js';
      }
      document.head.append(script)
    </script>

  </body>
</html>
